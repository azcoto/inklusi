{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "SERVER",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmit": true
  }
}
